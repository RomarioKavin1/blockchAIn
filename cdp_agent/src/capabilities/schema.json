{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "_id": {
        "type": "string",
        "format": "uuid",
        "coerce": true
      },
      "agent_name": {
        "type": "string"
      },
      "thread_id": {
        "type": "string"
      },
      "wallet_id": {
        "type": "string"
      },
      "network_id": {
        "type": "string"
      },
      "encrypted_seed": {
        "type": "string"
      },
      "created_at": {
        "type": "string",
        "format": "date-time",
        "coerce": true
      }
    },
    "required": [
      "_id",
      "agent_name",
      "thread_id",
      "wallet_id",
      "encrypted_seed"
    ],
    "additionalProperties": false
  }
}
